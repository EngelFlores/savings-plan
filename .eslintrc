{
  "extends": ["eslint:all", "plugin:react/all"],
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "func-names": ["error", "never"],
    "linebreak-style": 0,
    "indent": [
      "error",
      2,
      { "SwitchCase": 1 }
    ],
    "no-console": ["error", { "allow": ["error"] }],
    "semi": ["error", "always"],
    "quotes": ["error", "single", { "allowTemplateLiterals": true }],
    "one-var": 0,
    "react/jsx-indent": [2, 2],
    "react/jsx-indent-props": [2, 2],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/no-set-state": 0,
    "react/jsx-curly-spacing": [2, "always", {"spacing": {
      "objectLiterals": "never"
    }}],
    "react/no-multi-comp": [ "error", { "ignoreStateless": true }],
    "react/jsx-boolean-value": 0,
    "space-infix-ops": 0,
    "react/jsx-no-bind": 0,
    "react/forbid-prop-types": 0,
    "react/jsx-handler-names": 0,
    "react/forbid-component-props": 0,
    "react/function-component-definition": "off",
    "react/jsx-one-expression-per-line": "off",
    "react/jsx-no-literals": "off",
    "function-call-argument-newline": "off",
    "react/require-optimization": 0,
    "arrow-parens": "off",
    "class-methods-use-this": "off",
    "no-underscore-dangle": "off",
    "space-in-parens": [ "error", "always" ],
    "template-curly-spacing": [ "error", "always" ],
    "no-trailing-spaces": "error",
    "no-magic-numbers": "warn",
    "capitalized-comments": "off",
    "multiline-comment-style": "off",
    "id-length": "warn",
    "padded-blocks": "off",
    "consistent-return": "warn",
    "no-param-reassign": "warn",
    "array-element-newline": "off",
    "max-statements": [ "warn", 20 ],
    "max-lines-per-function": "off",
    "no-plusplus": "off",
    "no-ternary": "off",
    "prefer-destructuring": "off",
    "quote-props": "off",
    "object-property-newline": "off",
    "use-isnan": "error",
    "for-direction": "error",
    "getter-return": "error",
    "no-cond-assign": "error",
    "multiline-ternary": "off",
    "max-depth": "error",
    "arrow-body-style": "off",
    "no-extra-parens": "warn",
    "no-invalid-this": "warn",
    "react/state-in-constructor": "off",
    "react/jsx-max-depth": [ "error", { "max": 5 } ],
    "react/no-unescaped-entities": "off",
    "react/jsx-child-element-spacing": "off"
  },
  "env": {
    "node": true,
    "es6": true
  },
  "plugins": [
    "react"
  ],
  "globals": {
    "$": false,
    "afterEach": false,
    "alert": false,
    "atob": false,
    "beforeEach": false,
    "CHAVE_CRIPTO_ADYEN": false,
    "confirm": false,
    "DEBUG": false,
    "describe": false,
    "document": false,
    "expect": false,
    "expectMissingProp": false,
    "FastClick": false,
    "fbq": false,
    "fetch": false,
    "ga": false,
    "GOOGLE_ANALYTICS_ID": false,
    "Headers": false,
    "INTERCOM_ACCESS_TOKEN": false,
    "INTERCOM_ID": false,
    "Intercom": false,
    "it": false,
    "localStorage": false,
    "mount": false,
    "OEF_VERSION": false,
    "RECAPTCHA_SITE_KEY": false,
    "shallow": false,
    "sinon": false,
    "stubConsoleError": false,
    "Swiper": false,
    "test": false,
    "twq": false,
    "URL_INTERCOM": false,
    "window": false,
    "YT": false,
    "render": false,
    "LOCAL": false,
    "RECAPTCHA_ENABLED": false,
    "grecaptcha": false,
    "RECAPTCHA_SITE_KEY_FAWKES": false,
    "__voxusTrackConversion": false,
    "RAVEN_CONFIG_URL": false,
    "FACEBOOK_APP_ID": false,
    "API_BASE_URL": false,
    "SENTRY_ENVIRONMENT": false,
    "CANONICAL_URL": false,
    "STATIC_ASSETS_BASE_URL": false,
    "INVISIBLE_RECAPTCHA_KEY": false,
    "FACEBOOK_PIXEL_ID": false,
    "FIREBASE_API_KEY": false,
    "FIREBASE_AUTH_DOMAIN": false,
    "FIREBASE_DATABASE_URL": false,
    "FIREBASE_PROJET_ID": false,
    "FIREBASE_STORAGE_BUCKET": false,
    "FIREBASE_MESSAGING_SENDER_ID": false,
    "firebase": false,
    "MOBDIQ_CONFIG_KEY": false,
    "Mobdiq": false,
    "IS_PRODUCTION": false,
    "YBOX_URL": false,
    "MASTER_TAG_EFFILIATION_URL": false,
    "dataLayer": false,
    "sessionStorage": false,
    "navigator": false,
    "jest": false,
    "afterAll": false,
    "beforeAll": false
  }
}
